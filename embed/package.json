{
  "name": "@rexec/embed",
  "version": "1.0.0",
  "description": "Embeddable terminal widget for Rexec - Cloud Shell anywhere",
  "main": "dist/rexec.min.js",
  "module": "dist/rexec.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "terminal",
    "xterm",
    "embed",
    "cloud-shell",
    "rexec",
    "widget"
  ],
  "author": "Rexec",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rexec/rexec"
  },
  "homepage": "https://rexec.dev",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "terser": "^5.27.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "dependencies": {
    "@xterm/xterm": "^5.5.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-webgl": "^0.18.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
